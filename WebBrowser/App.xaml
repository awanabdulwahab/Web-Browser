<Application
    x:Class="WebBrowser.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:WebBrowser">

    <Application.Resources>
        
                   <!--Web Browser Custom Colors-->
        <SolidColorBrush x:Key="wbBackgroundColor" Color="#222831"></SolidColorBrush>
        <SolidColorBrush x:Key="wbLighterBlue" Color="#30475e"></SolidColorBrush>
        <SolidColorBrush x:Key="wbForegroundColor" Color="#f05454"></SolidColorBrush>
        <SolidColorBrush x:Key="wbLighterGrey" Color="#dddddd"></SolidColorBrush>
    </Application.Resources>

</Application>
